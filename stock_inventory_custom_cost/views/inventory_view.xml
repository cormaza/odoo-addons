<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="stock_inventory_line_tree2" model="ir.ui.view">
        <field name="name">stock.inventory.line.tree.view</field>
        <field name="model">stock.inventory.line</field>
        <field name="inherit_id" ref="stock.stock_inventory_line_tree2" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='theoretical_qty']" position="before">
                <field name="currency_id" invisible="1" />
                <field
                    name="custom_cost"
                    groups="stock_inventory_custom_cost.group_custom_cost_inventory"
                />
            </xpath>
        </field>
    </record>
</odoo>
